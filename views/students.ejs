<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Action</th>
    </tr>
    <% students.forEach((student)=>
        { %>
        <tr>
            <td>
                <%= student.student_id %>
            </td>
            <td>
                <%= student.student_name %>
            </td>
            <td>
                <%= student.age %>
            </td>
            <td>
                <a href="/students/delete/<%=student.student_id%>">Action</a>
            </td>
        </tr>
        <% }) %>
</table>